<?xml version="1.0" encoding="UTF-8"?>
<feature
      id="org.eclipselabs.mongo.emf.developer.junit.feature"
      label="MongoEMF JUnit Support"
      version="0.6.0.qualifier"
      provider-name="EclipseLabs"
      license-feature="org.eclipselabs.mongo.emf.feature">

   <description url="https://github.com/BryanHunt/mongo-emf/wiki">
      Mongo EMF JUnit developer bundles.  Install this feature into
your target platform if you want some utility functions that
make it easier to write JUnit tests involving EMF and MongoDB.
   </description>

   <copyright>
      Copyright (C) 2011 Bryan Hunt.  All rights reserved.
   </copyright>

   <license url="http://www.example.com/license">
      [Enter License Description here.]
   </license>

   <requires>
      <import plugin="com.mongodb"/>
      <import plugin="org.eclipse.emf.common"/>
      <import plugin="org.eclipse.emf.ecore"/>
      <import plugin="org.eclipselabs.mongo"/>
      <import plugin="org.eclipselabs.mongo.emf"/>
      <import plugin="org.hamcrest.core" version="1.1.0" match="greaterOrEqual"/>
      <import plugin="org.junit"/>
      <import plugin="org.eclipse.osgi"/>
      <import plugin="org.eclipselabs.emf.query.simple"/>
   </requires>

   <plugin
         id="org.eclipselabs.mongo.emf.developer.junit"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

</feature>
